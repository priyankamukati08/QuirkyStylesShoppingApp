Table
Products:

CREATE TABLE
IF NOT EXISTS public.PRODUCTS
(
id integer NOT NULL GENERATED ALWAYS AS IDENTITY
( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 2147483647 CACHE 1 ),
PRODUCT_BRAND_NAME VARCHAR
(255) NOT NULL ,
PRODUCT_DESCRIPTION VARCHAR
(255) NOT NULL ,
PRODUCT_CATEGORY  VARCHAR
(255) NOT NULL ,
PRODUCT_PRICE NUMERIC
(10,2) NOT NULL ,
PRODUCT_RATING NUMERIC
(10,1),
PRODUCT_TOTAL_QUANTITY NUMERIC NOT NULL,
create_date timestamp without time zone DEFAULT
(now
() AT TIME ZONE 'utc'::text), update_date timestamp without time zone DEFAULT
(now
() AT TIME ZONE 'utc'::text)
	)

CREATE TABLE
IF NOT EXISTS public.PRODUCTS_QUANTITY
(
id integer NOT NULL GENERATED ALWAYS AS IDENTITY
( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 2147483647 CACHE 1 ),
PRODUCT_SIZE VARCHAR
(255) NOT NULL ,
PRODUCT_SIZE_QUANTITY VARCHAR
(255) NOT NULL ,
create_date timestamp without time zone DEFAULT
(now
() AT TIME ZONE 'utc'::text), update_date timestamp without time zone DEFAULT
(now
() AT TIME ZONE 'utc'::text)
	)



CREATE TABLE
IF NOT EXISTS public.carts
(
id integer NOT NULL GENERATED ALWAYS AS IDENTITY
( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 2147483647 CACHE 1 ),
PRODUCT_QUANTITY NUMERIC NOT NULL,
create_date timestamp without time zone DEFAULT
(now
() AT TIME ZONE 'utc'::text), update_date timestamp without time zone DEFAULT
(now
() AT TIME ZONE 'utc'::text)
	)


CREATE TABLE
IF NOT EXISTS public.user_info
(
id integer NOT NULL GENERATED ALWAYS AS IDENTITY
( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 2147483647 CACHE 1 ),
USER_FULL_NAME VARCHAR
(255) NOT NULL ,
USER_email VARCHAR
(255) NOT NULL,
USER_PHONE_NUMBER VARCHAR
(20)  NOT NULL,
USER_AGE NUMERIC,
USER_GENDER VARCHAR
(255),
USER_SHIPPING_ADDRESS VARCHAR
(255) NOT NULL ,
USER_BILLING_ADDRESS VARCHAR
(255) NOT NULL ,
USER_Profile_Picture_URL  VARCHAR
(255),
cash_balance decimal,
create_date timestamp without time zone DEFAULT
(now
() AT TIME ZONE 'utc'::text), update_date timestamp without time zone DEFAULT
(now
() AT TIME ZONE 'utc'::text)
	)


CREATE TABLE
IF NOT EXISTS public.user_order
(
    id integer NOT NULL GENERATED ALWAYS AS IDENTITY
( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 2147483647 CACHE 1 ),
    user_id VARCHAR
(250) NOT NULL,
    status VARCHAR
(250) NOT NULL,
    order_payment_type VARCHAR
(250) NOT NULL,
    total_price DECIMAL,
    shipping_address VARCHAR
(255),
    billing_address VARCHAR
(255),
    payment_status VARCHAR
(50),
    delivery_status VARCHAR
(50),
    order_notes TEXT,
    create_date TIMESTAMP WITHOUT TIME ZONE DEFAULT
(now
() AT TIME ZONE 'utc'),
    update_date TIMESTAMP WITHOUT TIME ZONE DEFAULT
(now
() AT TIME ZONE 'utc')
);

CREATE TABLE
IF NOT EXISTS public.wishlist
(
        id integer NOT NULL GENERATED ALWAYS AS IDENTITY
( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 2147483647 CACHE 1 ),
    user_id VARCHAR
(250) NOT NULL,
    product_id INTEGER NOT NULL,
    date_added TIMESTAMP WITHOUT TIME ZONE DEFAULT
(now
() AT TIME ZONE 'utc'),
);


